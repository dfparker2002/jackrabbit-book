<ns = 'http://namespace.com/ns'>
<nw = 'http://kartashov.com/jackrabbit-cookbook/northwind'>

// ---------------------------------------------------------------------------------------------------------------------
// territories

[nw:territories] > nt:base
    + * (nw:region)
[nw:region] > nt:base
    - nw:regionDescription (string) mandatory
    + * (nw:territory) = nw:territory
[nw:territory] > nt:base, mix:referenceable
    - nw:territoryDescription (string) mandatory

// ---------------------------------------------------------------------------------------------------------------------
// employees

[nw:employees] > nt:base
    + * (nw:employee)
[nw:employee] > nt:base, mix:referenceable
    - nw:firstName (string) mandatory
    - nw:lastName (string) mandatory
    - nw:title (string)
    - nw:titleOfCourtesy (string)
    - nw:birthDate (date)
    - nw:hireDate (date)
    - nw:address (string)
    - nw:city (string)
    - nw:region (string)
    - nw:postalCode (string)
    - nw:homePhone (string)
    - nw:extension (string)
    - nw:photo (binary)
    - nw:notes (string)
    - nw:reportsTo (reference)
    - nw:photoPath (string)
    - nw:territories (reference) multiple

// ---------------------------------------------------------------------------------------------------------------------
// products

[nw:products] > nt:base
    + * (nw:category, nw:supplier, nw:shipper)
[nw:category] > nt:base, mix:referenceable
    - nw:categoryName (string) mandatory
    - nw:description (string)
    - nw:picture (binary)
[nw:supplier] > nt:base
    - nw:companyName (string) mandatory
    - nw:contactName (string)
    - nw:contactTitle (string)
    - nw:address (string)
    - nw:city (string)
    - nw:region (string)
    - nw:postalCode (string)
    - nw:country (string)
    - nw:phone (string)
    - nw:fax (string)
    - nw:homePage (string)
    + * (nw:product)
[nw:product] > nt:base, mix:referenceable
    - nw:productName (string) mandatory
    - nw:supplier (reference)
    - nw:category (reference)
    - nw:quantityPerUnit (string)
    - nw:unitPrice (double)
    - nw:unitsInStock (long)
    - nw:unityInOrder (long)
    - nw:recorderLevel (long)
    - nw:discontinued (boolean)
[nw:shipper] > nt:base, mix:referenceable
    - nw:companyName (string)
    - nw:phone (string)

// ---------------------------------------------------------------------------------------------------------------------
// customers

[nw:customers] > nt:base
    + * (nw:customer, nw:demographics)
[nw:customer] > nt:base, mix:referenceable
    - nw:companyName (string) = "" mandatory autocreated
    - nw:contactName (string)
    - nw:contactTitle (string)
    - nw:address (string)
    - nw:city (string)
    - nw:region (string)
    - nw:postalCode (string)
    - nw:country (string)
    - nw:phone (string)
    - nw:fax (string)
    - nw:demographics (reference) multiple
[nw:demographics] > nt:base, mix:referenceable
    - nw:description (string)

// ---------------------------------------------------------------------------------------------------------------------
// orders

[nw:orders] > nt:base
    + * (nw:order)
[nw:order] > nt:base
    - nw:customer (reference)
    - nw:employee (reference)
    - nw:orderDate (date)
    - nw:requiredDate (date)
    - nw:shippedDate (date)
    - nw:shipVia (reference)
    - nw:freight (double)
    - nw:shipName (string)
    - nw:shipAddress (string)
    - nw:shipCity (string)
    - nw:shipRegion (string)
    - nw:shipPostalCode (string)
    - nw:shipCountry (string)
    + * (nw:orderDetail)
[nw:orderDetail] > nt:base
    - nw:product (reference) mandatory
    - nw:unitPrice (double) mandatory
    - nw:quantity (long) mandatory
    - nw:discount (double) mandatory
