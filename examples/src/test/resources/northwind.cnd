<ns = 'http://namespace.com/ns'>
<nw = 'http://kartashov.com/jackrabbit-cookbook/northwind'>

// ---------------------------------------------------------------------------------------------------------------------
// territories

[nw:territories] > nt:base
    + * (nw:region)
[nw:region] > nt:base
    - regionDescription (string) mandatory
    + * (nw:territory) = nw:territory
[nw:territory] > nt:base, mix:referenceable
    - territoryDescription (string) mandatory

// ---------------------------------------------------------------------------------------------------------------------
// employees

[nw:employees] > nt:base
    + * (nw:employee)
[nw:employee] > nt:base, mix:referenceable
    - firstName (string) mandatory
    - lastName (string) mandatory
    - title (string)
    - titleOfCourtesy (string)
    - birthDate (date)
    - hireDate (date)
    - address (string)
    - city (string)
    - region (string)
    - postalCode (string)
    - homePhone (string)
    - extension (string)
    - photo (binary)
    - notes (string)
    - reportsTo (reference)
    - photoPath (string)
    - territories (reference) multiple

// ---------------------------------------------------------------------------------------------------------------------
// products

[nw:products] > nt:base
    + * (nw:category, nw:supplier, nw:shipper)
[nw:category] > nt:base, mix:referenceable
    - categoryName (string) mandatory
    - description (string)
    - picture (binary)
[nw:supplier] > nt:base
    - companyName (string) mandatory
    - contactName (string)
    - contactTitle (string)
    - address (string)
    - city (string)
    - region (string)
    - postalCode (string)
    - country (string)
    - phone (string)
    - fax (string)
    - homePage (string)
    + * (nw:product)
[nw:product] > nt:base, mix:referenceable
    - productName (string) mandatory
    - supplier (reference)
    - category (reference)
    - quantityPerUnit (string)
    - unitPrice (double)
    - unitsInStock (long)
    - unityInOrder (long)
    - recorderLevel (long)
    - discontinued (boolean)
[nw:shipper] > nt:base, mix:referenceable
    - companyName (string)
    - phone (string)

// ---------------------------------------------------------------------------------------------------------------------
// customers

[nw:customers] > nt:base
    + * (nw:customer, nw:demographics)
[nw:customer] > nt:base, mix:referenceable
    - companyName (string) = "" mandatory autocreated
    - contactName (string)
    - contactTitle (string)
    - address (string)
    - city (string)
    - region (string)
    - postalCode (string)
    - country (string)
    - phone (string)
    - fax (string)
    - demographics (reference) multiple
[nw:demographics] > nt:base, mix:referenceable
    - description (string)

// ---------------------------------------------------------------------------------------------------------------------
// orders

[nw:orders] > nt:base
    + * (nw:order)
[nw:order] > nt:base
    - customer (reference)
    - employee (reference)
    - orderDate (date)
    - requiredDate (date)
    - shippedDate (date)
    - shipVia (reference)
    - freight (double)
    - shipName (string)
    - shipAddress (string)
    - shipCity (string)
    - shipRegion (string)
    - shipPostalCode (string)
    - shipCountry (string)
    + * (nw:orderDetail)
[nw:orderDetail] > nt:base
    - product (reference) mandatory
    - unitPrice (double) mandatory
    - quantity (long) mandatory
    - discount (double) mandatory
